{
  "name": "blind-date",
  "version": "3.2.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "clean": "rimraf dist",
    "release": "pnpm clean && pnpm build && pnpm test:all && pnpx changeset publish && pnpm install",
    "release:version": "pnpm changeset version && pnpm install",
    "test": "jest",
    "test:all": "pnpm run test:nz && pnpm run test:us",
    "test:nz": "TZ=Pacific/Auckland jest",
    "test:us": "TZ=America/Los_Angeles jest",
    "watch": "tsc --watch"
  },
