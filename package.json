  "license": "Apache-2.0",
  "devDependencies": {
    "@changesets/cli": "^2.17.0",
    "@types/jest": "^26.0.24",
    "@types/luxon": "^1.27.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "dayjs": "^1.10.6",
    "eslint": "^7.30.0",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^27.0.6",
    "luxon": "^2.0.1",
    "moment": "^2.29.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cactuslab/blind-date.git"
  }
